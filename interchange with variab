#include <stdio.h>

int main()
{
    int a, b, temp;

    // Input two numbers
    printf("Enter first number (a): ");
    scanf("%d", &a);

    printf("Enter second number (b): ");
    scanf("%d", &b);

    // Before swapping
    printf("\nBefore swapping: a = %d, b = %d\n", a, b);

    // Swap using temporary variable
    temp = a;
    a = b;
    b = temp;

    // After swapping
    printf("After swapping: a = %d, b = %d\n", a, b);

    return 0;
}
